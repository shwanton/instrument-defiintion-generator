{
  "name": "instrument-defintion-generator",
  "author": "Shawn Dempsey <shwanton@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/generator",
      "packages/www"
    ]
  },
  "scripts": {
    "generate": "yarn workspace @packages/generator start",
    "test": "yarn workspace @packages/generator test",
    "parse": "yarn workspace @packges/generator parse",
    "www:start": "yarn workspace www start",
    "www:test": "yarn workspace www test",
    "compile": "tsc --build --force tsconfig.json",
    "clean": "rm -rf packages/www/build packages/generator/dist",
    "serve": "npx serve -s packages/www/build"
  },
  "devDependencies": {
    "jest": "^29.2.0",
    "jest-extended": "^3.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  }
}
